<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.somesh.somesh_2048.QUIZ_RESULT">

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/imageButton3"
        android:src="@drawable/bbb"
        android:scaleType="centerCrop"
        />

    <TextView
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:textSize="30sp"
        android:id="@+id/textView9"
        android:layout_marginTop="38dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:text="FACEBOOK"
        android:id="@+id/button9"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/textView9"
        android:layout_alignRight="@+id/button45"
        android:layout_alignEnd="@+id/button45" />

    <Button
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:text="TWITTER"
        android:id="@+id/button45"
        android:layout_marginTop="56dp"
        android:layout_below="@+id/button9"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:text="NETCAMP"
        android:id="@+id/button46"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/button45"
        android:layout_alignLeft="@+id/button45"
        android:layout_alignStart="@+id/button45" />

    <Button
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:text="HOME"
        android:id="@+id/button47"
        android:layout_below="@+id/button46"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp" />

    <WebView
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:id="@+id/webView"
        android:layout_below="@+id/button9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



</RelativeLayout>
